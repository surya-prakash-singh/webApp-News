{
  "name": "webapp-newsportal",
  "version": "1.0.0",
  "description": "A Simple News Search Portal",
  "main": "index.js",
  "scripts": {
    "cleanCache": "rm -rf ./node_modules/.cache/webpack",
    "clean": "rm -rf ./prod && npm run cleanCache",
    "start": "tsc && npm run lint && webpack serve --port 9000 --mode production --devtool inline-source-map --content-base ./public",
    "build": "npm run clean && npm run lint && tsc && webpack --mode production",
    "run:tsc": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts,.tsx --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/surya-prakash-singh/webApp-News.git"
  },
  "keywords": [
    "News",
    "Search"
  ],
  "author": "Surya Prakash Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/surya-prakash-singh/webApp-News/issues"
  },
  "homepage": "https://github.com/surya-prakash-singh/webApp-News#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
